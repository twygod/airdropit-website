/* Archive & Single
 */
.container.blog {
	padding-bottom: 250px;
}

@media (min-width: 1024px){
	.container.blog .content{ /*, .container.single .content*/
		padding: 0 5%;
	}
}
@media (min-width: 1280px){
	.container.blog .content{ /*, .container.single .content*/
		padding: 0 15%;
	}
}

.container.blog .content .entry {
	padding: 75px 0;
	border-bottom: 1px solid #dddddd;
}
.container.blog .content .entry.last{
	border: none;
}

.container.single .content .entry{
	padding: 75px 0;
}

.container.blog .content .quote{
	display: none;
}

.container.single .content .quote{
	padding: 0;
	padding-bottom: 45px;
}

/* Categories navigation */
.container.blog .content .categories .itcat{
	min-height: 50px;
	display: -moz-inline-stack;
	display: inline-block;
	vertical-align: top;
	zoom: 1;
	*display: inline;
	_height: 50px;
	padding: 15px 25px 8px 25px;
    border-bottom: 1px solid #dddddd;
	position: relative;
}

@media (max-width: 767px){
	.container.blog .content .categories .itcat{
		padding: 15px 10px 8px 10px;
	}
}

@media (min-width: 768px){
	.container.blog .content .categories .itcat.active:after{
		content: '';
		background-image: url('../images/triangle-white.jpg');
		background-repeat: no-repeat;
		position: absolute;
		width: 13px;
		height: 8px;
		bottom: -8px;
		left: 45%;
	}
}

.container.blog .content .categories .itcat a{
	color: #44535d;
	font-weight: 300;
	font-size: 20px;
}

.container.blog .content .categories .itcat a:hover{
	color: #000000;
}
.container.blog .content .categories .itcat a.current{
	font-weight: 400;
}

