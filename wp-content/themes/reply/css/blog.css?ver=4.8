/* Safari view */
.safari {
	margin-bottom: 23px;
}
.safari > div::before {
	content: '';
	height: 38px;
	background-color: #eae9ea;
	display: block;
	width: 100%;
	top: 0;
	left: 0;
	position: absolute;
	z-index: -1;
}
.safari > div {
	position: relative;
	padding: 12px 0 8px 13px;
	border-radius: 5px 5px 0 0;
	border: 1px solid #eae9ea;
}
.safari div > span {
	display: inline-block;
	font-size: 16px;
	vertical-align: top;
}
.safari div > span:nth-child(1){
	height: 12px;
	width: 12px;
	border-radius: 50%;
	background-color: #fc625d;
}
.safari div > span:nth-child(2){
	height: 12px;
	width: 12px;
	margin-left: 8px;
	border-radius: 50%;
	background-color: #fdbe40;
}
.safari div > span:nth-child(3){
	height: 12px;
	width: 12px;
	margin-left: 8px;
	border-radius: 50%;
	background-color: #34ca49;
}
.safari div > span:nth-child(4) {
	display: none;
	width: 24px;
	height: 24px;
	margin-left: 13px;
	position: relative;
	top: -5px;
	line-height: 24px;
	text-align: center;
	border-radius: 3px;
	background-color: #fefefe;
	box-shadow: 0 1px 0 0 rgba(0,0,0,.15);
}
.safari div > span:nth-child(4)::before {
	font-family: FontAwesome;
	content: '\f053';
	color: #595959;
}
.safari div > span:nth-child(5) {
	display: none;
	margin-left: 2px;
	position: relative;
	top: -5px;
	width: 24px;
	height: 24px;
	line-height: 24px;
	text-align: center;
	border-radius: 3px;
	background-color: #fefefe;
	color: #b2b2b2;
	box-shadow: 0 1px 0 0 rgba(0,0,0,.15);
}
.safari div > span:nth-child(5)::before {
	font-family: FontAwesome;
	content: '\f054';
	color: #b2b2b2;
}
.safari div > span:nth-child(6) {
	content: 'Reply.io';
	font-size: 15px;
	width: 50%;
	height: 22px;
	line-height: 22px;
	text-align: center;
	border-radius: 3px;
	background-color: #fefefe;
	color: #b2b2b2;
	box-shadow: 0 1px 0 0 rgba(0,0,0,.15);
	position: absolute;
	top: 8px;
	left: 50%;
	transform: translateX(-50%);
}
.safari div > span:nth-child(6)::before {
	font-family: FontAwesome;
	content: '\f0e2';
	position: absolute;
	right: 7px;
	top: 50%;
	transform: translate(-50%, -50%);
	color: #7e7e7e;
}
.safari div > span:nth-child(6)::after {
	content: 'Reply.io';
	position: absolute;
	left: 50%;
	top: 50%;
	transform: translate(-50%, -50%);
	color: #000;
}
.safari div > span:nth-child(7) {
	display: none;
	float: right;
	position: relative;
	top: 2px;
	width: 23px;
	height: 23px;
	background-color: #d3d3d3;
	border-top: 1px solid #adadad;
	border-left: 1px solid #adadad;
	text-align: center;
}
.safari div > span:nth-child(7)::before {
	position: absolute;
	left: 50%;
	top: 50%;
	transform: translate(-50%, -50%);
	font-family: FontAwesome;
	content: '\f067';
	color: #8b8a8b;
}
.safari div > span:nth-child(8) {
	display: none;
	float: right;
	position: relative;
	margin-right: 13px;
	top: -4px;
	width: 37px;
	height: 23px;
	border-radius: 3px;
	background-color: #fdfdfd;
	text-align: center;
	box-shadow: 0 1px 0px 0 rgba(0,0,0,.15);
}
.safari div > span:nth-child(8)::before {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
	font-family: FontAwesome;
	content: '\f24d';
	color: #585858;
}
.safari div > span:nth-child(9) {
	display: none;
	float: right;
	margin-right: 13px;
	position: relative;
	top: -4px;
	display: inline-block;
	width: 37px;
	height: 23px;
	border-radius: 3px;
	background-color: #fdfdfd;
	text-align: center;
	box-shadow: 0 1px 0 0 rgba(0,0,0,.15);
}
.safari div > span:nth-child(9)::before {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	font-family: FontAwesome;
	content: '\f08e';
	color: #585858;
}
.safari div a {
	margin-top: 2px;
	border: 1px solid #eae9ea;
	border-top: 0;
	margin-left: -12px;
	border-radius: 0 0 3px 3px;
}
@media (min-width: 768px) {
	.safari {
		padding: 0 10%;
	}
}
@media (min-width: 992px) {
	.safari {
		padding: 0 10%;
	}
	.safari div > span:nth-child(4),
	.safari div > span:nth-child(5),
	.safari div > span:nth-child(7),
	.safari div > span:nth-child(8),
	.safari div > span:nth-child(9) {
		display: inline-block;
	}
}
@media (min-width: 1280px) {
	.safari {
		padding: 0 15%;
	}
}