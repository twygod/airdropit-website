}

@media (max-width: 767px){
	.container.blog .content .entry .readfull .morelink{
		margin-top: 45px;
	}
	.container.blog .content .entry .readfull .morelink a{
		background-color: #53C180;
		color: #ffffff;
		display: block;
		width: 100%;
		padding: 18px 0px;
		text-align: center;
	}
	.container.blog .content .entry .readfull .morelink a:hover{
