/* Meta */
.container .content .entry .meta img.catimg{
	margin-bottom: 4px;
}

.container .content .entry .meta span.cat{
	padding-left: 8px;
}
.container .content .entry .meta span.cat.product,
.container .content .entry .meta span.cat.sales,
.container .content .entry .meta span.cat.marketing,
.container .content .entry .meta span.cat.staff{
	font-size: 16px;
}

.container .content .entry .meta span.cat.product,
.container .content .entry .relcat span.cat.product{
	color: #ec5642;
}
.container .content .entry .meta span.cat.sales,
.container .content .entry .relcat span.cat.sales{
	color: #ffcd61;
}
.container .content .entry .meta span.cat.marketing,
.container .content .entry .relcat span.cat.marketing{
	color: #4d9de0;
}
.container .content .entry .meta span.cat.staff,
.container .content .entry .relcat span.cat.staff{
	color: #86e657;
}

.container .content .entry .meta .date{
	color: #a7b3bc;
}

.container.blog .content .entry .title {
	padding-bottom: 10px;
}

.container.blog .content .entry .title i.glyphicon.glyphicon-pencil,
.container.single .content .entry .title i.glyphicon.glyphicon-pencil{
	font-size: 14px;
	color: #ff0000;
}

.container.blog .content .entry .title h1{
	font-size: 48px;
	font-weight: 900;
	margin-top: 40px;
}

.container.single .content .entry .title h1,
