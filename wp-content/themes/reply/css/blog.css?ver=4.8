}

@media (max-width: 991px){
	.container.blog .content .entry .title h1{
		text-align: center;
	}
}

.container.blog .content .entry .post{
	color: #000000;
    font-size: 17px;
    line-height: 1.5;
    font-weight: 400;
}
@media (max-width: 767px){
	.container.blog .content .entry .post{
		line-height: 1.8;
	}
}

.container.blog .content .entry .post img {
	display: none;
}
.container.blog .content .entry .post img:first-of-type {
	display: block;
}

.container.blog .content .entry .readfull{
	padding-top: 45px;
}

.container.blog .content .entry .readfull .ava,
.container.single .content .entry .meta .ava,
.container.blog .content .entry .readfull .credit,
.container.single .content .entry .meta .credit,
