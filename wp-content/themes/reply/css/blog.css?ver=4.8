.postnav .prev,
.postnav .next{
	display: none;
}
@media (min-width: 1140px){
	.postnav .prev,
	.postnav .next{
		display: block;
	}
	.postnav .prev a,
	.postnav .next a{
		padding: 15px 30px 0px 30px;
		position: absolute;
		top: 330px;
		-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=10)";
		filter: alpha(opacity=10);
		-moz-opacity: 0.1;
		-khtml-opacity: 0.1;
		opacity: 0.1;
		-webkit-transition: all 0.35s;
		-moz-transition: all 0.35s;
		-o-transition: all 0.35s;
		transition: all 0.35s;
	}
	.postnav .prev a{
		background-image: url('../images/arrow-left.png');
		background-repeat: no-repeat;
		left: 70px;
	}
	.postnav .next a{
		background-image: url('../images/arrow-right.png');
		background-repeat: no-repeat;
		right: 70px;
	}
	.postnav .prev a:hover,
	.postnav .next a:hover{
		-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
		filter: alpha(opacity=80);
		-moz-opacity: 0.8;
		-khtml-opacity: 0.8;
		opacity: 0.8;
		-webkit-transform: scale(1.1);
		-moz-transform: scale(1.1);
		-ms-transform: scale(1.1);
		-o-transform: scale(1.1);
		transform: scale(1.1);
	}
	.postnav .prev.nomore,
	.postnav .next.nomore {
		display: none;
	}
}

.subscribe-up {
	margin: 0 auto;
	text-align: center;
	border-bottom: 1px solid #e8e8e8;

	position: fixed;
	top: -1920px;
	width: 100%;
	overflow: hidden;
	padding-top: 20px;
	background-color: #ffffff;
	z-index: 9999;
	border: none;
	-webkit-box-shadow: 0 0px 7px 0 #ddd;
	-moz-box-shadow: 0 0px 7px 0 #ddd;
	box-shadow: 0 0px 7px 0 #ddd;
	-webkit-transition: all 0.4s;
	-moz-transition: all 0.4s;
	-o-transition: all 0.4s;
	transition: all 0.4s;
}

.subscribe-up.visible {
	top: 0;
}

@media (max-width: 767px){
	.subscribe-up .hint{
		display: none;
	}
}

.subscribe-up div{
	margin-left: 5px;
	margin-right: 5px;
	line-height: 38px;
	min-height: 50px;
	_height: 50px;
	display: -moz-inline-stack;
	display: inline-block;
	vertical-align: top;
	zoom: 1;
	*display: inline;
}

@media (min-width: 768px){
	.subscribe-up div{
		min-height: 60px;
		_height: 60px;

	}
}

.subscribe-up .glyphicon{
	display: none;
	color: #cad6df;
	font-size: 16px;
}
@media (min-width: 992px){
	.subscribe-up .glyphicon{
		display: block;
	}
}

.subscribe-up a#close{
    position: absolute;
    right: 50px;
    top: 35px;
}

.subscribe-up input{
	background-color: #f6f7f8;
	padding: 0px 14px;
	height: 38px;
	outline: none !important;
	border: 1px solid #cad6df;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}

.subscribe-up .btn.btn-success{
	background-color: #3dcd8d;
	height: 38px;
	outline: none !important;
	box-shadow: none !important;
	border: none;
    line-height: 26px;
    padding: 6px 30px;
	margin-bottom: -1px;
}

@media (max-width: 767px){
	.subscribe-up{
		padding-top: 10px;
	}
	.subscribe-up .ibut{
		width: 30%;
	}
	.subscribe-up .isub{
		width: 60%;
	}
	.subscribe-up input,
	.subscribe-up .btn.btn-success{
		width: 100%;
		/*padding-left: 20%;*/
	}
	.subscribe-up div{
		margin-left: 0;
	}
}

.subscribe-up .btn.btn-success:hover,
.subscribe-up .btn.btn-success:focus,
.subscribe-up .btn.btn-success:active{
	background-color: #00a65c;
}

