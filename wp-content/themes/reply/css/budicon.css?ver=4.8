@charset "UTF-8";

@font-face {
	font-family: "budicon2";
	src:url("../fonts/budicon2.eot");
	src:url("../fonts/budicon2.eot?") format("embedded-opentype"),
		url("../fonts/budicon2.woff") format("woff"),
		url("../fonts/budicon2.ttf") format("truetype"),
		url("../fonts/budicon2.svg") format("svg");
	font-weight: normal;
	font-style: normal;
}

[data-icon]:before {
	font-family: "budicon2" !important;
	content: attr(data-icon);
	font-style: normal !important;
	font-weight: normal !important;
	font-variant: normal !important;
	text-transform: none !important;
	speak: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
	font-family: "budicon2" !important;
	font-style: normal !important;
	font-weight: normal !important;
	font-variant: normal !important;
	text-transform: none !important;
	speak: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

